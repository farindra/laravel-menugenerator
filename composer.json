{
    "name": "farindra/laravel-menugenerator",
    "description": "drag and drop menu generator like wordpress for laravel",
    "keywords": ["Laravel menu", "Menu", "Wordpress", "Builder"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/farindra/laravel-menugenerator"
        }
    ],
    "authors": [
        {
            "name": "Farindra",
            "email": "farindra.project@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": ">=5"
    },
    "autoload": {
        "psr-4": {
          "Farindra\\Menugenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Farindra\\Menugenerator\\MenugeneratorServiceProvider"
            ],
            "aliases": {
                "Menugenerator": "Farindra\\Menugenerator\\Facades\\Menugenerator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
